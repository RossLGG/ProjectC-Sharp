@model VentasOrganosCortes.Models.ViewModels.TablaViewModel7
@{
    ViewBag.Title = "Nuevo";
}

<h2>@ViewBag.Title</h2>


@using (Html.BeginForm("Nuevo", "Compra", FormMethod.Post))
{
    <div class="row">
        <div class="col-md-12">
            @Html.LabelFor(x => x.Id)
            @Html.TextBoxFor(x => x.Id, "", new { @class = "form-control" })
            @Html.LabelFor(x => x.Fecha)
            @Html.TextBoxFor(x => x.Fecha, "", new { @class = "form-control" })
            @Html.LabelFor(x => x.Id_Clie)
            @Html.TextBoxFor(x => x.Id_Clie, "", new { @class = "form-control" })
            @Html.LabelFor(x => x.Id_Medio)
            @Html.TextBoxFor(x => x.Id_Medio, "", new { @class = "form-control" })

            <div style="text-align:left;padding:10px;">
                <input value="Guardar" type="submit" class="btn btn-primary" />
            </div>
        </div>
    </div>
}